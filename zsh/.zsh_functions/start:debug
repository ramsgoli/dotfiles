start:debug() {
  HOSTNAME=$(hostname -s)
  if [[ $HOSTNAME != "rams-work-mbp" ]]; then
    echo "Not your work laptop. WTF?"
    return
  fi

  CURRENT_DIR=$(pwd)
  if [[ $CURRENT_DIR != "/Users/ram/heap/heap" ]]; then
    echo "Must be run from ~/heap/heap"
    return
  fi
  LOG_LEVEL=debug npm run start:app
}
